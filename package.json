{
  "name": "k-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:dev": "rimraf dist && rollup --bundleConfigAsCjs --config scripts/rollup/dev.config.js",
    "demo": "vite serve demos/test-fc --config scripts/vite/vite.config.js --force",
    "lint": "eslint ./packages",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.4",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "commitlint": "^19.8.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.37.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript-eslint": "^8.28.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "vite": "^3.2.3"
  },
  "pnpm": {
    "overrides": {
      "react": "link:dist/node_modules/react",
      "react-dom": "link:dist/node_modules/react-dom"
    }
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-replace": "^6.0.2",
    "react": "link:dist/node_modules/react",
    "react-dom": "link:dist/node_modules/react-dom"
  }
